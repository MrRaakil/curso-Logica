algoritmo "estrutura-aninhada"
var
   Din : real
inicio
      Escreva ("quanto temos??? ")
      Leia (Din)
      
      Se (Din >= 10000) Entao
         Escreva ("Partiu Disney!")
      Senao
           Se (Din >= 2000) e (Din < 10000) Entao
              Escreva ("Vou Pra Itamaravilha")
           Senao
                 Escreva("Netflix and chill")
           FimSe
      FimSe
fimalgoritmo
